{
  "shell": {
    "name": "SyndraShell",
    "version": "1.0.0"
  },
  "appearance": {
    "theme": "catppuccin",
    "bar_position": "Top",
    "dock_position": "Bottom",
    "animations": true,
    "blur": true,
    "shadows": true
  },
  "hyprland": {
    "border_size": 2,
    "gaps_in": 5,
    "gaps_out": 10,
    "rounding": 10
  },
  "apps": {
    "terminal": "kitty",
    "launcher": "wofi",
    "file_manager": "thunar",
    "browser": "firefox"
  },
  "keybinds": {
    "prefix_dash": "SUPER",
    "suffix_dash": "D",
    "prefix_launcher": "SUPER",
    "suffix_launcher": "R",
    "prefix_restart": "SUPER ALT",
    "suffix_restart": "B"
  }
}
